	stage('Build')
	{
	echo "It is bulding the project"
	}
	
	stage('Test')
	{
	echo "It is Testing the project"
	}
	
	stage('Deploy')
	{
	echo "It is Deploying the project"
	}
	
	stage('Release')
	{
	echo "It is Releasing the project"
	}
	
	stage('Monitor')
	{
	echo "It is Monitoring the project"
	}
}
